{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:jest/recommended"], 
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-console": "error",
        "semi" : "off"
    }
}

